public class BrppTeste {

	public static String[] lines = { 
			"uso Motor;",
			"uso Brava.Leitor;",
			
			"Fabrica Teste{",
			"	imprimaln('algo');",
			"	Restrito Numero num;",
			"	SemRetorno Configuracao(){",
			"	}",
			"	Numero alfa = 0;",
			"	NumeroDecimal beta = 1;",
			"	Numero soma(){",
			"		Resposta alfa+beta;",
			"	}",
			"	Palavra palavra = 'caraca';",
			"	Condicao teste = Verdade; ",
			"	Numero x = 1;",
			"	enquanto (x>0) faça{",
			"		x++;	}",
			"	Numero gamma;",
			"	se (teste) faça {",
			"		NumeroDecimal soma = alfa+beta;"
			+ "	}",
			"}" };

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		if (BrppCompiler.compile(lines) == false)
			System.out.println("Seu código contém erros");
	}

}
